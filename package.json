{
  "name": "@udia/udia",
  "version": "0.2.1",
  "description": "Encrypted file and note storage, with collaboration",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "NODE_ENV=test yarn runMigrations",
    "test": "NODE_ENV=test TYPEORM_LOGGING=false jest --verbose",
    "posttest": "yarn lint",
    "prestart": "yarn clean && yarn build && yarn runMigrations",
    "start": "node dist/index.js",
    "build": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "watch": "nodemon --watch src --verbose --ext ts --exec yarn start",
    "prepublishOnly": "yarn test",
    "makeMigration": "ts-node node_modules/.bin/typeorm migration:generate",
    "runMigrations": "ts-node node_modules/.bin/typeorm migration:run",
    "revertMigration": "ts-node node_modules/.bin/typeorm migration:revert",
    "dropSchema": "ts-node node_modules/.bin/typeorm schema:drop",
    "clean": "rm -rf dist/*",
    "cleanLogs": "rm -rf log/*.log"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/testHelper.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.ts",
      "!**/src/migration/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/udia-software/udia.git"
  },
  "homepage": "https://api.udia.ca",
  "author": {
    "name": "Alexander Wong",
    "email": "alex@udia.ca",
    "url": "https://udia.ca"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "@types/cors": "^2.8.3",
    "@types/express": "^4.11.1",
    "@types/express-jwt": "^0.0.40",
    "@types/graphql": "~0.12.7",
    "@types/ioredis": "^3.2.7",
    "@types/jest": "^22.2.0",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/node": "^9.4.7",
    "@types/node-fetch": "^1.6.9",
    "@types/nodemailer": "^4.6.0",
    "@types/on-finished": "^2.3.1",
    "@types/pg": "^7.4.5",
    "@types/websocket": "^0.0.38",
    "@types/winston": "^2.3.8",
    "@types/ws": "^4.0.2",
    "@udia/graphql-postgres-subscriptions": "^1.0.0",
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.1",
    "apollo-link-http": "^1.5.3",
    "apollo-link-ws": "^1.0.7",
    "apollo-server-express": "^1.3.4",
    "apollo-utilities": "^1.0.11",
    "argon2": "^0.18.3",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-redis-subscriptions": "^1.5.0",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^2.23.1",
    "iterall": "^1.2.2",
    "jest": "^22.4.3",
    "jsonwebtoken": "^8.2.0",
    "moment": "^2.21.0",
    "node-fetch": "^2.1.2",
    "node-graceful": "^0.3.0",
    "nodemailer": "^4.6.4",
    "nodemon": "^1.17.2",
    "on-finished": "^2.3.0",
    "pg": "^7.4.1",
    "pg-native": "^2.2.0",
    "pjson": "^1.0.9",
    "reflect-metadata": "^0.1.12",
    "subscriptions-transport-ws": "^0.9.7",
    "ts-jest": "^22.4.2",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "typeorm": "^0.2.0",
    "typescript": "^2.7.2",
    "websocket": "^1.0.25",
    "winston": "^2.4.1",
    "ws": "^5.1.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.0"
  },
  "engines": {
    "node": ">=8.11.1 <10"
  },
  "os": [
    "darwin",
    "linux"
  ]
}
