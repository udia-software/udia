    "cookie-parser": "^1.4.3",
    "errorhandler": "^1.5.0",
    "express": "^4.14.0",
    "method-override": "^2.3.7",
    "mongoose": "^4.6.8",
    "morgan": "^1.7.0",
    "pug": "^2.0.0-beta6",
    "q": "^1.4.1"
  },
