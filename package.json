{
  "name": "@udia/udia",
  "version": "0.7.0",
  "description": "Encrypted item storage.",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "NODE_ENV=test yarn runMigrations",
    "test": "NODE_ENV=test TYPEORM_LOGGING=false jest --verbose --detectOpenHandles",
    "posttest": "yarn lint",
    "prestart": "yarn clean && yarn build && yarn runMigrations",
    "start": "node dist/index.js",
    "build": "tsc",
    "lint": "tslint -c tslint.json --project .",
    "watch": "nodemon --watch src --verbose --ext ts --exec yarn start",
    "prepublishOnly": "yarn test",
    "makeMigration": "ts-node node_modules/.bin/typeorm migration:generate",
    "runMigrations": "ts-node node_modules/.bin/typeorm migration:run",
    "revertMigration": "ts-node node_modules/.bin/typeorm migration:revert",
    "dropSchema": "ts-node node_modules/.bin/typeorm schema:drop",
    "clean": "rimraf dist",
    "cleanLogs": "rimraf log coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "src/__tests__/.*(test|spec)\\.(jsx?|tsx?)$",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "node_modules/*",
      "src/__tests__/testHelper.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/__tests__/*",
      "!src/migration/*",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/udia-software/udia.git"
  },
  "homepage": "https://api.udia.ca",
  "author": {
    "name": "Alexander Wong",
    "email": "alex@udia.ca",
    "url": "https://udia.ca"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/express-jwt": "^0.0.40",
    "@types/graphql": "^14.0.3",
    "@types/ioredis": "^4.0.3",
    "@types/jest": "^23.0.0",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/node": "^10.0.3",
    "@types/node-fetch": "^2.1.0",
    "@types/nodemailer": "^4.6.0",
    "@types/on-finished": "^2.3.1",
    "@types/pg": "^7.4.5",
    "@types/serve-index": "^1.7.29",
    "@types/websocket": "^0.0.40",
    "@types/winston": "^2.3.8",
    "@types/ws": "^6.0.1",
    "@udia/graphql-postgres-subscriptions": "^1.0.0",
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.1",
    "apollo-link-http": "^1.5.3",
    "apollo-link-ws": "^1.0.7",
    "apollo-server-express": "^2.1.0",
    "apollo-utilities": "^1.0.11",
    "argon2": "^0.19.0",
    "aws-sdk": "^2.235.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "~6.1.0",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^4.0.2",
    "iterall": "^1.2.2",
    "jest": "^23.0.1",
    "jsonwebtoken": "^8.2.0",
    "moment": "^2.21.0",
    "node-fetch": "^2.1.2",
    "node-graceful": "^0.3.0",
    "nodemailer": "^4.6.4",
    "nodemon": "^1.17.2",
    "on-finished": "^2.3.0",
    "pg": "^7.4.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "serve-index": "^1.9.1",
    "subscriptions-transport-ws": "^0.9.7",
    "ts-jest": "^23.0.0",
    "ts-node": "^7",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "typeorm": "^0.2.0",
    "typescript": "^3.1.3",
    "websocket": "^1.0.25",
    "winston": "^3",
    "ws": "^6.1.0"
  },
  "engines": {
    "node": ">=8.12 <=11"
  },
  "os": [
    "darwin",
    "linux"
  ]
}
